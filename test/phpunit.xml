<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="./../vendor/autoload.php" convertErrorsToExceptions="true" convertNoticesToExceptions="true" convertWarningsToExceptions="true">
    <testsuites>
        <testsuite name="Pipeline">
            <directory>./src</directory>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist>
            <directory suffix=".php">./../src</directory>
        </whitelist>
    </filter>
    <logging>
        <log type="coverage-html" target="./coverage" lowUpperBound="35" highLowerBound="70"/>
    </logging>
</phpunit>
